{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "compile and run",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "preLaunchTask": "Compile TypeScript",
            "program": "${workspaceFolder}/dist/parser/test.js",
            "outFiles": [
                "${workspaceFolder}/**/*.js"
            ]
        },
        {
            "type": "node-terminal",
            "name": "JavaScript Debug Terminal",
            "request": "launch",
            "cwd": "${workspaceFolder}"
        }
    ]
}